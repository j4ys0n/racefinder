            <div class="content">
                <div class="header">
                    <img class="logo" src="/public/Includes/images/logo.jpg" height="50"/>
                    <div class="controls">
                        <input type="text" class="dateStart datePicker"/>
                        <input type="text" class="dateEnd datePicker"/>
                        <select class="raceType" disabled>
                            <option value="1">Road Races</option>
                            <option value="2">CX</option>
                            <option value="3">MTB</option>
                            <option value="6">Track</option>
                        </select>
                        <button class="searchBtn">Search</button>
                    </div>
                </div>
                <div class="map-canvas"></div>
                <div class="anchor">
                    <a href="https://twitter.com/share" class="twitter-share-button"
                     data-lang="en" data-url="http://racefinder.bike" data-size="8"
                     data-text="map based bike race finder." data-via="jayssface"
                     data-count="none" data-counturl="http://racefinder.bike">Tweet</a>
                    feedback: <a href="http://twitter.com/jayssface">@jayssface</a>
                </div>
            </div>
        </div>
