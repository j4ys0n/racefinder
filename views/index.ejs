<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, minimum-scale=0.5, maximum-scale=1, initial-scale=1, user-scalable=no, target-densitydpi=device-dpi, minimal-ui">

        <title>racefinder.bike</title>

        <meta name="title" content="racefinder.bike" />
    	<meta name="description" content="Map-based bike race finder. See what's near you!" />
    	<meta property="og:locale" content="en_US"/>
    	<meta property="og:url" content="http://racefinder.bike" />
    	<meta property="og:title" content="racefinder.bike" />
        <meta property="og:description" content="Map-based bike race finder. See what's near you!" />
    	<meta property="og:type" content="website" />
    	<meta property="og:image" content="http://racefinder.bike/public/Includes/images/racefinder-preview.png" />
    	<meta property="og:site_name" content="racefinder"/>
    	<meta name="twitter:url" content="http://racefinder.bike">
    	<meta name="twitter:card" content="summary">
    	<meta name="twitter:creator" content="@https://twitter.com/jayssface">
    	<meta name="twitter:site" content="@jayssface">

	    <link rel="shortcut icon" href="public/Includes/images/icon.png" type="image/png" />
        <link rel="icon" type="image/png" href="public/Includes/images/icon.png" />
        <link href="public/Includes/css/styles.css" media="all" rel="stylesheet" type="text/css">
        <link href="//code.jquery.com/ui/1.10.4/themes/smoothness/jquery-ui.css" media="all" rel="stylesheet" type="text/css">
    </head>
    <body class="on-load-ask-server" data-action="getRaces({
            url: /api/races/date/1/4%2F16%2F14/06%2F01%2F2014 })">
        <div class="container">
            <div class="content">
                <div class="header">
                    <img class="logo" src="/public/Includes/images/logo.jpg" height="50"/>
                    <div class="controls">
                        <span class="dateStack">
                            <input type="text" class="dateStart datePicker"/>
                            <input type="text" class="dateEnd datePicker"/>
                        </span>
                        <span class="locationStack">
                            <span class="location overlay-open" data-overlay="location" title="Change your location">New York, NY</span>
                            <input type="text" class="distance" placeholder="miles"/>
                        </span>


                        <ul class="race-type group">
                            <li>
                                <input id="radio-road" type="radio" name="race-type" value="1" checked/>
                                <label for="radio-road">
                                    <span class="icon radio-road"></span>Road
                                </label>
                            </li>
                            <li>
                                <input id="radio-cx" type="radio" name="race-type" value="2"/>
                                <label for="radio-cx">
                                    <span class="icon radio-cx"></span>CX
                                </label>
                            </li>
                            <li>
                                <input id="radio-mtb" type="radio" name="race-type" value="3"/>
                                <label for="radio-mtb">
                                    <span class="icon radio-mtb"></span>MTB
                                </label>
                            </li>
                            <li>
                                <input id="radio-track" type="radio" name="race-type" value="6"/>
                                <label for="radio-track">
                                    <span class="icon radio-track"></span>Track
                                </label>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="map-canvas"></div>
                <div class="anchor">
                    <a href="https://twitter.com/share" class="twitter-share-button"
                     data-lang="en" data-url="http://racefinder.bike" data-size="8"
                     data-text="map based bike race finder." data-via="jayssface"
                     data-count="none" data-counturl="http://racefinder.bike">Tweet</a>
                    feedback: <a href="http://twitter.com/jayssface">@jayssface</a>
                </div>
            </div>
            <div class="overlays">
                <div class="overlay location">
                    <span class="close expand-hit-area">&times;</span>
                    <input type="text" class="input-location" placeholder="Enter your location"/>
                    <span class="btn">Set Location</span>
                </div>
            </div>
        </div>
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
        <script src="//code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
        <script type="text/javascript"
              src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAXPwfiflEHVhuTLPWrb7j1d5PMyXS-UeE&sensor=true"></script>
        <script src="public/Includes/scripts/AppGlobal.js"></script>
        <script src="public/Includes/scripts/Scrape.js"></script>
        <script src="public/Includes/scripts/Map.js"></script>
        <script src="public/Includes/scripts/Overlay.js"></script>
        <script src="public/Includes/scripts/bootstrap.js"></script>

        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-50131112-1', 'racefinder.bike');
          ga('send', 'pageview');

          !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="https://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");

        </script>
    </body>
</html>
